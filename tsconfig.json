{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "jsx": "preserve",
    "lib": ["DOM", "DOM.Iterable", "ES2021"],
    "module": "esnext",
    "moduleDetection": "force",
    "moduleResolution": "nodenext",
    "noEmit": true,
    "resolveJsonModule": false,
    "skipLibCheck": true,
    "strict": false,
    "target": "ES2021"
  }
}
